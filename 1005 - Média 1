def calcular_media_ponderada(nota_a, nota_b):
  """Calcula a média ponderada de duas notas.

  Args:
    nota_a: A primeira nota.
    nota_b: A segunda nota.

  Returns:
    A média ponderada.
  """

  peso_a = 3.5
  peso_b = 7.5
  media = (nota_a * peso_a + nota_b * peso_b) / (peso_a + peso_b)
  return media

nota_a = float(input())
nota_b = float(input())


media = calcular_media_ponderada(nota_a, nota_b)


print(f"MEDIA = {media:.5f}")
